{
  "name": "@pleaseai/asana",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "description": "Asana CLI - Manage your Asana tasks from the command line",
  "author": {
    "name": "Minsu Lee",
    "email": "amond@amond.net",
    "url": "https://github.com/amondnet"
  },
  "license": "MIT",
  "homepage": "https://github.com/pleaseai/asana#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/pleaseai/asana.git"
  },
  "bugs": {
    "url": "https://github.com/pleaseai/asana/issues"
  },
  "keywords": [
    "asana",
    "cli",
    "task-management",
    "productivity",
    "command-line",
    "asana-api"
  ],
  "bin": {
    "asana": "./src/index.ts"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "dev:secure": "dotenvx run -- bun run src/index.ts",
    "build": "bun build src/index.ts --compile --outfile asana",
    "test": "bun test test/",
    "test:unit": "bun test test/",
    "test:e2e": "bun test tests/e2e",
    "test:e2e:secure": "dotenvx run -- bun test tests/e2e",
    "test:all": "bun test",
    "test:watch": "bun test test/ --watch",
    "test:coverage": "bun test test/ --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "env:encrypt": "dotenvx encrypt",
    "env:decrypt": "dotenvx decrypt"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.51.0",
    "asana": "^3.1.2",
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "open": "^10.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.0.0",
    "@types/bun": "^1.3.1",
    "eslint": "^9.37.0",
    "eslint-plugin-format": "^1.0.2",
    "turbo": "^2.5.8"
  }
}
